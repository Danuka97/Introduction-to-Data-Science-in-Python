   "source": [
    "## Question 1\n",
    "For this question, calculate the win/loss ratio's correlation with the population of the city it is in for the **NHL** using **2018** data."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "deletable": false,
    "nbgrader": {
     "checksum": "1cac4803b02502929f5b1612d48db2b5",
     "grade": false,
     "grade_id": "cell-69b16e4386e58030",
     "locked": false,
     "schema_version": 1,
     "solution": true
    }
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "0.012486162921209907"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import scipy.stats as stats\n",
    "import re\n",
    "\n",
    "nhl_df=pd.read_csv(\"assets/nhl.csv\")\n",
    "cities=pd.read_html(\"assets/wikipedia_data.html\")[1]\n",
    "cities=cities.iloc[:-1,[0,3,5,6,7,8]]\n",
    "\n",
    "def nhl_correlation(): \n",
    "    pd.set_option('display.max_rows', None)\n",
    "    cities=pd.read_html(\"assets/wikipedia_data.html\")[1]\n",
    "    cities=cities.iloc[:-1,[0,3,5,6,7,8]]\n",
    "    cities.columns = ['Metropolitan area', 'Population', 'NFL','MLB','NBA','NHL']\n",
    "\n",
    "    # remove special characters with regex\n",
    "\n",
    "    cities['NHL'] = cities['NHL'].str.replace(r\"\\[.*\\]\",\"\")\n",
    "\n",
    "    # extract all team names, make one column of them and join with cities + population + remove nans\n",
    "\n",
    "    NHLt = cities['NHL'].str.extract('([A-Z]{0,2}[a-z0-9]*)([A-Z]{0,2}[a-z0-9]*)([A-Z]{0,2}[a-z0-9]*)') #extract\n",
    "    NHLt = NHLt.replace('Red','Red Wings')\n",
    "    NHLt = NHLt.replace('Maple','Maple Leafs')\n",
    "    NHLt = NHLt.replace('Blue','Blue Jackets')\n",
    "    NHLt = NHLt.replace('Golden','Golden Knights')\n",
    "    NHLt['Metropolitan area'] = cities['Metropolitan area'] \n",
    "    NHLt = pd.melt(NHLt, id_vars=['Metropolitan area']).drop(columns=['variable']).dropna().replace(\"\",np.nan).replace(\"—\",np.nan)\n",
    "    NHLt = NHLt.dropna()\n",
    "    NHLt = NHLt.merge(cities, how = 'left', on = 'Metropolitan area').iloc[:,:3]\n",
    "    NHLt.columns = ['Metropolitan area', 'NHL','Population']\n",
    "\n",
    "    ###\n",
    "\n",
    "    nhl_df=pd.read_csv(\"assets/nhl.csv\")\n",
    "    nhl_df = nhl_df[nhl_df['year'] == 2018]\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace(r'\\*',\"\")\n",
    "    nhl_df = nhl_df[['team','W','L']]\n",
    "    nhl_df = nhl_df[~nhl_df.W.str.contains(\"Division\")]\n",
    "\n",
    "    nhl_df['team'] = nhl_df['team'].str.extract('(?:[A-Z][a-z]*)(.*)')\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('York',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('Bay',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('Jersey',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('York',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('Louis',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('Jose',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('Angeles',\"\")\n",
    "    nhl_df['team'] = nhl_df['team'].str.replace('.',\"\")\n",
    "\n",
    "    nhl_df = nhl_df.astype({'W': int, 'L': int, 'team': str})\n",
    "    NHLt = NHLt.astype({'Metropolitan area': str, 'NHL': str, 'Population': int})\n",
    "    nhl_df['WLR'] = nhl_df['W']/(nhl_df['W']+nhl_df['L'])\n",
    "\n",
    "    nhl_df = nhl_df.apply(lambda x: x.str.strip() if x.dtype == \"object\" else x)\n",
    "    nhl_df = nhl_df.apply(lambda x: x.str.lower() if x.dtype == \"object\" else x)\n",
    "    NHLt = NHLt.apply(lambda x: x.str.strip() if x.dtype == \"object\" else x)\n",
    "    NHLt = NHLt.apply(lambda x: x.str.lower() if x.dtype == \"object\" else x)\n",
    "    nhl_df.columns = ['NHL','W','L','WLR']\n",
    "\n",
    "    nhl_merge = NHLt.merge(nhl_df, 'outer', on = 'NHL').dropna()\n",
    "\n",
    "    corr_nhl = nhl_merge.groupby('Metropolitan area').agg({'WLR': np.nanmean, 'Population': np.nanmean})\n",
    "\n",
    "    corr_nhl = corr_nhl.reset_index()\n",
    "    #corr_nhl = nhl_merge.merge(corr_nhl, 'outer', on = 'Metropolitan area')[['Metropolitan area', 'WLR_y','Population']]\n",
    "    corr_nhl.columns = ['Area', 'WLR', 'Pop']\n",
    "    corr_nhl = corr_nhl.astype({'Area': str, 'WLR': float, 'Pop': float})\n",
    "\n",
    "    corr_nhl\n",
    "\n",
    "    population_by_region = corr_nhl['Pop']\n",
    "    win_loss_by_region = corr_nhl['WLR']\n",
    "\n",
    "\n",
    "    win_loss_by_region\n",
    "    population_by_region\n",
    "\n",
    "    stats.pearsonr(population_by_region, win_loss_by_region)\n",
    "\n",
    "    assert len(population_by_region) == len(win_loss_by_region), \"Q1: Your lists must be the same length\"\n",
    "    assert len(population_by_region) == 28, \"Q1: There should be 28 teams being analysed for NHL\"\n",
    "    \n",
    "    return stats.pearsonr(population_by_region, win_loss_by_region)[0]\n",
    "\n",
    "nhl_correlation()"
   ]
  },
